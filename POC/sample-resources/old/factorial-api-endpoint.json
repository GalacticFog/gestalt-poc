{
    "resource_type": "Gestalt::Resource::ApiEndpoint",
    "name": "/factorial-example",
    "description": "Factorial Example Endpoint",
    "properties": {
        "resource": "/factorial-example",
        "methods": [
            "GET",
            "PUT",
            "POST",
            "DELETE"
        ],
        "plugins": {
            "rateLimit": {
                "enabled": false,
                "perMinute": 60
            },
            "gestaltSecurity": {
                "enabled": false,
                "users": [

                ],
                "groups": [

                ]
            }
        },
        "synchronous": true,
        "implementation_type": "lambda",
        "implementation_id": "#{Lambda factorial}"
    },
    "context": {
        "api": {
            "id": "#{Api dev-sandbox}"
        }
    }
}
